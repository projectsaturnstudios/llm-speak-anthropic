{
    "name": "llm-speak/anthropic-claude",
    "description": "A Laravel package for integrating Anthropic Claude into LLMSpeak",
    "type": "library",
    "version": "0.4.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Project Saturn Studios, LLC",
            "email": "info@projectsaturnstudios.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {},
    "autoload":{
        "psr-4": {
            "LLMSpeak\\Anthropic\\": "src/"
        }
    },
    "autoload-dev":{
        "psr-4": {}
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "LLMSpeak\\Anthropic\\Providers\\AnthropicLLMSpeakServiceProvider"
            ]
        }
    }
}
